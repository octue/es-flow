@article{Ashton2013,
abstract = {Resource assessments for marine renewable energy are commonly provided by a combination of wave measurement and modelling. For most sites, a long-term wave data set is not available. Instead, wave model output is validated against shortterm wave measurements, or those taken nearby. A hindcast is then used to give an extended data set, allowing a statistically robust assessment of the wave climate at the site. Often, these models will not effectively resolve intra-site variability. This paper describes analysis of the spatial properties of ocean waves at a marine renewables site scale. This draws on unique measurements from an array of four directional wave buoys installed in a square formation, with side 500m, in the Celtic Sea, UK, and a high spatial resolution spectral wave model covering the same site. Here we show that the both the measurements and the model predict differences in the wave climate within the site. These results highlight the importance of intra-site variability for marine renewable energy and suggest that high spatial resolution spectral wave models, perhaps nested into a larger model domain, have the potential to provide a data set for the prediction of spatial variability of wave conditions across a marine renewable energy site.},
author = {Ashton, Ian G C and Nieuwkoop, Joana Van and Smith, Helen C M and Johanning, Lars and Smith, George H},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Ashton et al. - 2013 - Detailed resource assessment , including intra-site variability , for marine renewable energy.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {marine renewable energy,resource assessment,spatial variability,swan,wave measurement},
title = {{Detailed resource assessment , including intra-site variability , for marine renewable energy}},
year = {2013}
}
