@article{Stark2013,
abstract = {Oceanographic, geotechnical and sedimentological surveys were carried out in Grand Passage, Bay of Fundy, for environmental assessment with regard to the proposed in- stallation of tidal energy converters. Here, we present results obtained using the dynamic penetrometer Nimrod for rapid geotechnical characterization of the seafloor surface, and median grain sizes determined from grab samples. Sediment probing was complemented by sequential bathymetric surveying using multibeam sonar. The latter revealed a complex bathymetry including dune fields corresponding to a strong variability of sediments (gravel to fine sand, and shell hash) and geotechnical appearance of the seafloor surface. The Nimrod profiles ranged from very hard impacts, over a soft top layer of 2–7cm thickness above a stiffer substratum, to sites of poor consolidation in the upper decimeter of the seafloor surface. Comparison with multibeam images allowed identification of ongoing sediment remobilization processes. Additionally, this data set was used to assess the WASSP R ? system's capability of estimating surficial seafloor hardness by comparison to the Nimrod results. Overall, the results of our investigations to date suggest ongoing processes of sediment remobilization, which potentially impact on the in- stream tidal power development in this passage.},
author = {Stark, Nina and Hay, Alex E and Trowse, Greg and Kopf, Achim},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Stark et al. - 2013 - Geotechnical investigation of Grand Passage , Nova Scotia , with regard to sediment mobility and the installation.pdf:pdf},
journal = {Proceedings of the European Wave and Tidal Energy Conference 2013},
pages = {1--10},
title = {{Geotechnical investigation of Grand Passage , Nova Scotia , with regard to sediment mobility and the installation of tidal energy converters}},
year = {2013}
}
@article{Starzmann2013,
abstract = {This paper presents a full-scale testing methodology of SCHOTTEL´s STG 50 horizontal free-flow turbine, featuring a rotor diameter of 4m. The goal of this work is to demonstrate the functionality of the entire power take-off system using an intermediate step between model scale testing and offshore installation. In order to simulate a wide range of flow conditions within a short testing period, a tailored test rig has been designed and mounted in front of a harbour tug boat. The complete measurement setup is presented, including an empirical inflow velocity correction to account for the non-uniform flow field in front of the tug boat. All tests have been performed in the harbour of Rotterdam. For various flow velocities, measurements on power and thrust characteristics are performed and the results displayed for a quasi-rigid and a passive-adaptive rotor blade set. Eventually the measurements are compared with predictions from a 3D RANS simulation. Further performance analysis with the control strategy applied, proves the thrust reduction potential of the passive-adaptive blades in overspeed conditions.},
author = {Starzmann, R and Baldus, M and Groh, E and Hirsch, N and Lange, N A and Scholl, S},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Starzmann et al. - 2013 - Full-Scale Testing of a Tidal Energy Converter Using a Tug Boat(2).pdf:pdf},
journal = {10th European Wave and Tidal Energy Conference (EWTEC 2013), Aalborg, Dk, 02 - 05 Sep 2013},
keywords = {cfd,full-scale,horizontal axis turbine,passive-adaptive blades,testing,tidal energy},
pages = {1--8},
title = {{Full-Scale Testing of a Tidal Energy Converter Using a Tug Boat}},
year = {2013}
}
