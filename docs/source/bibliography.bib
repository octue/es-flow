@article{Divett2013,
abstract = {Tidal stream turbines will be deployed in arrays to operate economically that will need to be optimally arranged for maximum energy extraction. Large Eddy Simulations are used to explore the channel scale optimisation and tuning of large array layout. Adaptive mesh and idealisations reduced the simulation time, allowing simulation of hundreds of tuned layouts with reasonable computational cost. The flow is driven by an elevation gradient, so the velocity reduces as turbines are added, allowing comparison of large arrays that remove a significant fraction of the natural velocity. If the array is restricted to only partially block the channel's width then the optimum layout depends on the number of partial rows in the array. Turbines should be packed densely on one side of the channel for one row and less densely packed for more rows. More total power is captured as rows are added to the array although the power that each turbine captures reduces as rows are added. The adaptive approach bridges the gap between large and small scale models of arrays and may be the first paper to numerically model array layout with partial rows or tuning at a channel scale.},
author = {Divett, Tim A and Vennell, Ross and Stevens, Craig},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Divett, Vennell, Stevens - 2013 - Channel scale tuning of large tidal turbine arrays using large eddy simulations with adaptive mesh.pdf:pdf},
