@article{Halliday2013,
abstract = {The development of an environmental hydrodynamic model is a key step in the site design and calculation of energy yield for marine renewable projects. For a tidal energy site the model provides the means to spatially and temporally predict the energy yield of devices located away from fixed measurement locations. In setting up a numerical model engineers have control over three significant inputs: bathymetry, boundary conditions and bed roughness. Determining the location of boundary conditions is a subjective matter that can either be tackled on a trial and error basis or as a conservative issue setting boundaries beyond a continental shelf. As the tidal energy industry progresses the uncertainty with which hydrodynamic models are able to predict conditions, and hence energy yield, is coming under increased scrutiny. In order to provide evidence as to the uncertainty associated with predicted values the quality of input data needs to be assessed. This paper presents the results of comparing a high resolution (0.125x0.125deg) global ocean model to predicted tidal heights, generated by the United Kingdom Hydrographic Office, across the North West European Shelf. The mapped results are intended to provide a reference as to the quality and placement of boundary conditions for the qualitative assessment of energy yield predictions.},
author = {Halliday, J Ross},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Halliday - 2013 - Defining Hydrodynamic Boundaries for Reduced Modelling Uncertainty.pdf:pdf},
