keywords = {marine renewable energy,open-sea test facilities,risk based,safety considerations,technical requirements},
title = {{Safety Considerations for the Deployment of Marine Renewable Energy Devices at Biscay Marine Energy Platform (bimep)}},
year = {2013}
}
