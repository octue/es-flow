@article{Easton2013,
abstract = {Tidal stream turbines are frequently incorporated within ocean and basin scale numerical models in an attempt to quantify the available resource and characterise the environmental response to energy extraction. The fundamental principle behind tidal stream energy extraction via horizontalaxis turbines is straightforward - the kinetic energy of the moving fluid is transferred to the turbine rotor. Describing the complexities of this process is more challenging - energy extraction is non-linear in current speed. Furthermore, inefficiencies in turbine operation, drag on the turbine support structure and mixing in the turbine wake all dissipate energy that is not reflected in electricity generation. For arrays of multiple turbines these additional energy sinks are not negligible. Consequently, there exists a potential dissonance between site developers and regulators as there is a fundamental difference between licensing the level of electricity generation and the total level of energy extraction. In this paper, we simulate an array of tidal stream turbines in the Pentland Firth, Scotland. A two-dimensional hydrodynamic model provides a useful and reliable description of this environment. A parameterisation of tidal stream turbines accounts for the non-linear response of turbine output to current speed and the influence of turbine support structure. We show that energy dissipation by an array can significantly exceed power production. Accounting for turbine non-linearity at high current speeds, however, limits the magnitude of the peak environmental stress. Implications for array modelling are discussed from the perspective of performing environmental impact assessment.},
author = {Easton, Matthew C and Woolf, David K},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Easton, Woolf - 2013 - The influence of non-linear turbine dynamics on the environmental stress of tidal stream arrays.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {environmental impact assessment,found,from numerical modelling,numerical model,overall good agreement with,pentland firth,several case studies have,these analytical theories,tidal array,tidal turbines},
number = {2},
title = {{The influence of non-linear turbine dynamics on the environmental stress of tidal stream arrays}},
year = {2013}
}
