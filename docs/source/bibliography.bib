@article{Joubert2013,
abstract = {This paper presents a study to select an appropriate location for the ShoreSWEC wave energy converter to be incorporated in a new breakwater and to design and test the chamber layout of the device. In order to identify a suitable port development on the South African coast appropriate site selection criteria were developed based on the construction and operational requirements of the device. Once an appropriate site was selected the spectral wave model, SWAN, was used to transfer 11 years of hindcast NCEP wave data from deep sea to the selected site. Results of the wave modeling procedure were validated through comparison with measured wave data near the site. The operational wave conditions were found to be an average wave height of 0.5 m with an 11 s wave period from predominately the northwest direction and the mean annual average wave power approximately 2.3 kW/m. The hundred year design storm conditions were found to be a significant wave height of 4.94 m and peak wave period of 17 s, also from the northwest. The stability of the device was evaluated under the wave loads caused by these design conditions. The formulae of Goda [1] were used to estimate the pressure distribution and corresponding forces and overturning moments acting on the vertical and inclined front wall of the device. It was found that an 18 m wide (5.6 m capture chamber and 12.4 m ballast chamber), 13.2 m long and 15.1 m high unit provided sufficient stability under design load conditions. Preliminary results from tank tests are presented to evaluate the conversion efficiency of the device at the selected location.},
author = {Joubert, James R and van Niekerk, Johannes L},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Joubert, Niekerk - 2013 - Design of a Novel Breakwater Wave Energy Converter for the South African Coast.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {africa,caisson breakwater,energy converter,owc,prevailing,resource assessment,shoreswec,south africa,southwest coast of south,swec,was developed for the,wave,wave conditions on the},
title = {{Design of a Novel Breakwater Wave Energy Converter for the South African Coast}},
year = {2013}
}
@article{Kadiri2013,
abstract = {The potential impacts of a barrage on the eutrophic status of the Severn Estuary were investigated in this study using a simple modelling approach adopted by the UK's Comprehensive Studies Task Team (CSTT). The model predictions were compared against CSTT thresholds for assessing eutrophication in coastal waters. Under both pre- and post-barrage (ebb-only generation) conditions, the estuary was found to be potentially eutrophic as the predicted equilibrium nutrient concentrations and potential maximum phytoplankton biomass exceeded the CSTT thresholds. Potential primary productivity under post-barrage conditions was found to be significantly higher than that under pre-barrage conditions. This is believed to be due to a combination of increased water residence time and greater light availability behind the barrage.},
author = {Kadiri, Margaret and Kay, David and Ahmadian, Reza and Bockelmann-Evans, Bettina and Falconer, Roger},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Kadiri et al. - 2013 - Potential impacts of a barrage on the eutrophic status of the Severn Estuary , UK.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {barrage,estuary,eutrophication,nutrients,primary productivity,severn},
title = {{Potential impacts of a barrage on the eutrophic status of the Severn Estuary , UK}},
year = {2013}
}
