}
@article{Faudot2013,
abstract = {As tidal turbine prototypes are likely to be installed at many locations around the world, it is important to identify the different load cases and especially the extreme loads on the blades such as those occurring in a runaway situation. In normal conditions, the rotor converts the kinetic energy of the water stream into mechanical energy, which is in turn converted into electrical energy by the generator. Runaway occurs when this last step is suddenly missing, following the loss of the connection to the output grid. The hydrodynamic torque on the rotor is then unbalanced and the rotor accelerates. The measurements carried out in a towing tank agrees with the CFD simulation and the quasi-static blade element momentum theory (BEM) on the large angular acceleration when the turbine is released, on the settled angular velocity (runaway speed) and on the steady thrust force equal to almost twice the operating thrust. But the quasi-static BEM is not able to capture dynamic effects, and thus predict the high peaks in angular velocity and thrust force. The structural integrity of the blades is then threatened by both the high stresses related to the large rotor acceleration and the jump in thrust force, arising from the increase in tip speed ratio and significant dynamic effects. This runaway load case is expected to be higher for tidal turbines than wind turbines due to the lower moment of inertia of the rotor and the high density of water. The object of this paper is to estimate the forces acting on the blades in runaway situation using an experimental and a numerical approach (CFD computation with ANSYS-CFX).},
author = {Faudot, Celine and Dahlhaug, Ole G and Holst, Martin a},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Faudot, Dahlhaug, Holst - 2013 - Tidal turbine blades in runaway situation experimental and numerical approaches.pdf:pdf},
journal = {10th European Wave and Tidal Energy Conference (EWTEC 2013), Aalborg, Dk, 02 - 05 Sep 2013},
title = {{Tidal turbine blades in runaway situation : experimental and numerical approaches}},
year = {2013}
}
