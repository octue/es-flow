@article{Kamizuru2013,
abstract = {Efficient and robust power take-offs (PTO) are needed for wave energy converters to become a commercially viable technology. Fluid power is a favourable solution since Hydro-static Drive Trains (HDT) are well proven and mass produced. This paper provides examples of Hydrostatic Drive Trains for wave energy converters (WEC) with different control strategies (e. g. fixed force, linear damping and reactive control) describes their operational behaviour and efficiency. It is shown how requirements for force control and energy storage influence system design and overall efficiency. Subsequently a test rig of a Hydrostatic Drive Train for WEC is presented and results for step response and efficiency are discussed. Dynamic wave-to-wire (W2W) simulations – of a heaving buoy and a bottom hinged flap – taking into account the efficiency of the PTO components are introduced to assess and optimise the performance. As locations for the flap Yakutat (Alaska, USA) and for the heaving buoy Reedsport (Oregon, USA) are chosen. In order to enter discussions, the results of the simulations are presented in the form of matrices containing power output, capture factors and PTO efficiency.},
author = {Kamizuru, Yukio and Fissmann, Christian and Murrenhoff, Hubertus},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Kamizuru, Fissmann, Murrenhoff - 2013 - Hydrostatic Drive Trains for Wave Energy Converters Simulation and Experiments for Efficient De.pdf:pdf},
