@article{headandbandyopadhyay1981,
abstract = {Flow visualization studies of the zero-pressure-gradient turbulent boundary layer over the Reynolds-number range 500 {\textless} Re, {\textless} 17500 have shown large Reynolds- number effects on boundary-layer structure. At high Reynolds numbers (Re, {\textgreater} 2000, say) the layer appears to consist very largely of elongated hairpin vortices or vortex pairs, originating in the wall region and extending through a large part of the boundary-layer thickness or beyond it; they are for the most part inclined to the wall at a characteristic angle in the region of 40-50'. Large-scale features, which exhibit a slow overturning motion, appear to consist mainly of random arrays of such hairpin vortices, although there is some evidence of more systematic structures. At low Reynolds numbers (Re, {\textless} 800, say) the hairpin vortices are very much less elongated and are better described as horseshoe vortices or vortex loops ; large-scale features now consist simply of isolated vortex loops (at the very lowest Reynolds numbers), or of several such loops interacting strongly, and show a relatively brisk rate of rotation.},
address = {40 WEST 20TH STREET, NEW YORK, NY 10011-4211},
author = {Head, M. R. and Bandyopadhyay, P.},
doi = {10.1017/S0022112081001791},
issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
number = {-1},
pages = {297},
publisher = {CAMBRIDGE UNIV PRESS},
title = {{New aspects of turbulent boundary-layer structure}},
type = {Article},
volume = {107},
year = {1981}
}
