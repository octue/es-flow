keywords = {Boundary layer stability,Entropy,Transition onset},
number = {4},
pages = {375--387},
title = {{On the use of entropy to predict boundary layer stability}},
volume = {6},
year = {2004}
}
@article{Wang2013,
abstract = {Marine energy, as one type of renewable energy, have developed rapidly and are about to play a big role in the new energy field in recent years. Nowadays, more attention was paid on better use of ocean energy all over the world. In the process of development and utilization of ocean energy, lots of tidal power farms are under construction for the purpose of commercial application. In order to gain more tidal energy, one of the best ways is to put multiple tidal turbines together to make tidal power farm. These turbines in tidal power farm are not isolated, they are interacted each other. Tidal current turbine array which is consisted of a large number of turbines will have more effect on the flow field around it, and then have influent on energy capture efficiency. Among all the parameters that have impact on turbines in tidal farms, the rotation direction of turbine rotor has influent on the performance of turbines. In this paper, by using the method of numerical simulation, two kinds of blade models for bi-directional turbine were established to study how rotation direction of rotor affects the flow field and the force on blades. This will help us find the optimized layout of turbines in tidal power farm, to make good use of the beneficial effect between turbines and reduce the adverse factors, and this synergistic effect will lead to the turbines capture more power than that of the same number of isolated machines. This research work in this paper will provide theoretical basis for the construction of tidal power station in the future.},
author = {Wang, Shujie and Li, Linjie and Tan, Junzhe and Yuan, Peng},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Wang et al. - 2013 - The Influence on the Performance of Horizontal Axis Tidal Turbines in Tidal Power Farm Considering the Rotor Rotati.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {numerical simulations,rotation direction of rotor,tidal power farm,turbine layout},
title = {{The Influence on the Performance of Horizontal Axis Tidal Turbines in Tidal Power Farm Considering the Rotor Rotation Direction}},
year = {2013}
}
