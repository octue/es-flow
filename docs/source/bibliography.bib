author = {Carlier, Johan and Stanislas, Michel},
doi = {10.1017/S0022112005004751},
issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
month = {jul},
pages = {143--188},
publisher = {CAMBRIDGE UNIV PRESS},
title = {{Experimental study of eddy structures in a turbulent boundary layer using particle image velocimetry}},
type = {Article},
volume = {535},
year = {2005}
}
@article{Carlson2013,
abstract = {The deployment of tidal turbines in coastal waters raises questions about the potential risk to marine animals from strike by rotating blades. Of particular concern are marine mammals that are already facing threats from other human activities as well as climate change. Regulators in the US who are charged with permitting the installation of tidal turbines have sought additional information to guide biological assessments of blade strike to marine mammals. This study determined a “worst case” scenario for interaction between a marine mammal and a tidal turbine, focusing on the highly endangered Southern Resident Killer Whale in the U.S. and an open-centred tidal turbine (OpenHydro) proposed for deployment. The analysis combined a finite element modelling of the forces from the turbine blade with information on the biomechanical analysis of the marine mammal tissues, in order to estimate the potential results of a blade strike. Supported by ancillary data, this analysis is being used to inform permitting of a tidal energy project in Puget Sound, Washington, U.S. Analyses are underway to further develop this and related analyses to include three tidal turbine designs and three marine mammal species.},
author = {Carlson, Thomas and Jepsen, Rich and Copping, Andrea},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Carlson, Jepsen, Copping - 2013 - Potential Effects of the Interaction between Marine Mammals and Tidal Turbines – An Engineering and.pdf:pdf},
journal = {Paper Presented at the European Wave and Tidal Energy Conference, Aalborg, Denmark.},
keywords = {analysis,biomechanical tissue properties,finite element,marine mammals,southern resident,tidal energy},
pages = {8},
title = {{Potential Effects of the Interaction between Marine Mammals and Tidal Turbines – An Engineering and Biomechanical Analysis}},
year = {2013}
}
