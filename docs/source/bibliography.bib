@article{novaraetal2010,
abstract = {A novel technique to increase the accuracy of multiplicative algebraic reconstruction technique$\backslash$n(MART) reconstruction from tomographic particle image velocimetry (PIV) recordings at higher seeding$\backslash$ndensity than currently possible is presented. The motion tracking enhancement (MTE) method is based$\backslash$non the combined utilization of images from two or more exposures to enhance the reconstruction of$\backslash$nindividual intensity fields. The working principle is first introduced qualitatively, and the$\backslash$nmathematical background is given that explains how the MART reconstruction can be improved on the$\backslash$nbasis of an improved first guess object obtained from the combination of non-simultaneous views$\backslash$nreduced to the same time instant deforming the 3D objects by an estimate of the particle motion$\backslash$nfield. The performances of MTE are quantitatively evaluated by numerical simulation of the imaging,$\backslash$nreconstruction and image correlation processes. The cases of two or more exposures obtained from$\backslash$ntime-resolved experiments are considered. The iterative application of MTE appears to significantly$\backslash$nimprove the reconstruction quality, first by decreasing the intensity of the ghost images and$\backslash$nsecond, by increasing the intensity and the reconstruction precision for the actual particles. Based$\backslash$non computer simulations, the maximum imaged seeding density that can be dealt with is tripled with$\backslash$nrespect to the MART analysis applied to a single exposure. The analysis also illustrates that the$\backslash$nmaximum effect of the MTE method is comparable to that of doubling the number of cameras in the$\backslash$ntomographic system. Experiments performed on a transitional jet at Re = 5000 apply the MTE method to$\backslash$ndouble-frame recordings. The velocity measurement precision is increased for a system with fewer$\backslash$nviews (two or three cameras compared with four cameras). The ghost particles' intensity is also$\backslash$nvisibly reduced although to a lesser extent with respect to the computer simulations. The velocity$\backslash$nand vorticity field obtained from a three-camera reconstruction with MTE are equivalent to that from$\backslash$na four-camera analysis. Possible variants of the MTE algorithm are investigated based on a first$\backslash$nguess obtained by average or by product of pseudo-simultaneous objects (PSO), which potentially$\backslash$noffer a higher convergence rate.},
address = {DIRAC HOUSE, TEMPLE BACK, BRISTOL BS1 6BE, ENGLAND},
author = {Novara, Matteo and Batenburg, Kees Joost and Scarano, Fulvio},
doi = {10.1088/0957-0233/21/3/035401},
issn = {0957-0233},
journal = {Measurement Science and Technology},
month = {mar},
number = {3},
pages = {035401},
publisher = {IOP PUBLISHING LTD},
title = {{Motion tracking-enhanced MART for tomographic PIV}},
type = {Article},
volume = {21},
year = {2010}
}
