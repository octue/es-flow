@article{davidsonetal2006b,
abstract = {It has been shown recently that the real-space equivalent of the k?1$\backslash$nlaw for near-wall turbulence is a logarithmic law for the second-order$\backslash$nlongitudinal structure function, ?(?u)2?(r) = u*2(A+B?ln(r/y)). Here$\backslash$ny is the distance from the wall, u* is the shear velocity, A and$\backslash$nB are coefficients of order unity and r is measured in the streamwise$\backslash$ndirection. In this paper we provide theoretical arguments to suggest$\backslash$nthat, in the limit of large Reynolds number, B is a universal constant$\backslash$nwhile A is of the form A = A??B?ln(P/?), where A? is a universal$\backslash$nconstant and P and ? are the rates of production and dissipation$\backslash$nof energy, respectively. Hence A is a weak, universal, function of$\backslash$ny. Two independent sets of data are examined and it is shown that,$\backslash$nto within experimental error, our predictions are consistent with$\backslash$nthe data.},
address = {CIRCULATION {\&} FULFILLMENT DIV, 2 HUNTINGTON QUADRANGLE, STE 1 N O 1, MELVILLE, NY 11747-4501 USA},
author = {Davidson, P. a. and Krogstad, P. {\AA} and Nickels, T. B.},
doi = {10.1063/1.2214087},
isbn = {1070-6631},
issn = {10706631},
journal = {Physics of Fluids},
month = {jun},
number = {6},
publisher = {AMER INST PHYSICS},
title = {{A refined interpretation of the logarithmic structure function law in wall layer turbulence}},
type = {Article},
volume = {18},
year = {2006}
}
@article{davidsonetal2006a,
abstract = {The k(-1) spectral law for near-wall turbulence has received only limited experimental support, the most convincing evidence being that of Nickels et al. (Phys. Rev. Lett. vol. 95, 2005, 074501.1). The real-space analogue of this law is a logarithmic dependence on r of the streamwise longitudinal structure function. We show that, unlike the k(-1) law, the logarithmic law is readily seen in the experimental data. We argue that this difference arises from the finite value of Reynolds number in the experiments. Reducing the Reynolds number is equivalent to restricting the range of eddy sizes which contribute to the k(-1), or lnr, laws. While the logarithmic law is relatively insensitive to a truncation in the range of eddy sizes (it continues to hold over the relevant range of eddy sizes), it turns out that the k(-1) law is not. This is a direct consequence of the so-called aliasing problem associated with one-dimensional spectra, whereby energy is systematically and artificially displaced to small wavenumbers.},
address = {40 WEST 20TH ST, NEW YORK, NY 10011-4211 USA},
author = {Davidson, P. a. and Nickels, T. B. and Krogstad, P.-{\AA}.},
doi = {10.1017/S0022112005008001},
isbn = {0022-1120},
issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
month = {mar},
number = {-1},
pages = {51},
publisher = {CAMBRIDGE UNIV PRESS},
title = {{The logarithmic structure function law in wall-layer turbulence}},
type = {Article},
volume = {550},
year = {2006}
}
