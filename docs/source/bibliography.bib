@article{Sarkar2013,
abstract = {Recent results have shown that flap-type wave energy converters are serious contenders in the challenge of harvesting energy from ocean water waves. In recent years, several analytical, numerical and experimental models have been made available to describe the behaviour of such converters in the open ocean and in an array. However, less interest has been paid in analysing the effects of the surrounding coastal environment on the performance of flap-type converters. This paper investigates the hydrodynamics of a flap-type wave energy converter near a straight coast line, modelled as an impermeable vertical cliff. Assuming the fluid inviscid and the flow irrotational, a potential flow theory is devised based on the application of Green's theorem in the fluid domain. This yields a hypersingular integral equation for the velocity potential which is solved via a series expansion in terms of the Chebyshev polynomials of the second kind. It is shown that resonance phenomena arise which can either increase or decrease the capture factor of the converter, depending on the ratio between the distance of the converter from the coast and the wavelength of the incident wave field.},
author = {Sarkar, Dripta and Renzi, Emiliano and Dias, Frederic},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Sarkar, Renzi, Dias - 2013 - Wave Power Absorption from a Flap-type Wave Energy Converter Near a Straight Coast.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {Oscillating Wave Surge Converter,Point Absorber.,Wave energy,flap-type converter,standing,waves},
title = {{Wave Power Absorption from a Flap-type Wave Energy Converter Near a Straight Coast}},
year = {2013}
}
