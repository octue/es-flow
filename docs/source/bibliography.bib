@article{Ocampo-Torres2013,
abstract = {Temporal and spatial variability of the wave field are key issues in the assessment of wave energy resources, specially when we need to focus in local areas. Nevertheless, long term tendencies and results from wave climate projections are also considered of relevance to schedules and plan for operation and maintenance. Long-term wave spectra simulation and shortperiod in situ records of the wave field are used to determine the variability of the most important wave characteristics in order to study their possible influence on the estimated energy potentially available in the Pacific coastal region off the Baja California peninsula in Mexico. A third generation spectral wave model (WAM) has been applied over practically the whole Pacific Ocean, using NCEP surface wind re-analysis as forcing fields, and hindcast results are obtained for oceanic scale runs (1o x 1o resolution). Smaller scale and finer resolution results are obtained by nested grids and the use of the SWAN model. Unstructured grids were further used with the SWAN model in the coastal region for local scale simulations. An important issue is the wider range of frequencies resolved by the models compared with standard runs already reported elsewhere, in order to better reproduce relevant processes in coastal regions such as non-linear interactions and changes in directional spreading of the wave spectrum. Detailed analysis of time variability of the wave field is performed and the potential impact on the estimation of extreme values is assessed. Long term tendencies of wave parameters are discussed within the context of the oceanic scale simulations of the wave phenomenon and their implications on the wave energy resources assessment is also addressed. Significant wave height variability is associated with decadal climate variations and other low frequency oscillation indicators such as ENSO (El Ni˜no Southern Oscillation), NAOI (North Atlantic Oscillation Index) and NPOI (North Pacific Oscillation Index). Long term tendencies of significant wave heights are analyzed in order to determine the effect of the time series lengths being used as well as discriminating among mean, extreme and maximum significant wave heights.},
author = {Ocampo-Torres, Francisco J and Osuna, Pedro and Rivera, Erick and Garcia, Ixetl and Ju{\`{a}}rez-Diaz, Tenoch I},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Ocampo-Torres et al. - 2013 - On the wave energy resource assessment in the Baja California coastal region and a study of the long term.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
