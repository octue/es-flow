@article{Caskey2013,
abstract = {The wave termination potential of a Cycloidal Wave Energy Converter is investigated by means of its wave generation characteristics. This is achieved by modelling a two dimensional hydrofoil rotating near a free surface using unsteady Reynolds Averaged Navier-Stokes (URANS) simulation. A grid refinement study is simultaneously completed using a coarse, medium and fine mesh at pitch angles of 0° and 10°. The study found that a medium sized mesh captures critical wave generation properties while minimizing computational requirements. The optimal mesh is then implemented at pitch angles of -10°, -5°, and 5°. For all simulations the hydrofoil generates a significant one-sided wave field consisting of a primary wave and its harmonics. The fundamental wave has a period equal to the device period and its amplitude increases linearly with pitch angle. A key difference between previous inviscid and the current URANS results is that these simulations are able to predict the detailed viscous flow field around the hydrofoil and resulting hydrodynamic forces for a fixed pitch angle. A decrease in outward radial force and an increase in tangential force were noted as the hydrofoil passed near the free surface for all cases. The hydrofoil surface pressure distribution varied significantly with circumferential position due to interactions with the free surface for positive pitch angles.},
author = {Caskey, Christopher and Jeans, Tiger},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Caskey, Jeans - 2013 - Analysis of a Cycloidal Wave Energy Converter Using Unsteady Reynolds Averaged Navier-Stokes Simulation.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {averaged navier-stokes simulation,cycloidal wave,energy converter,mesh refinement study,ocean wave energy conversion,unsteady reynolds,wave generation},
title = {{Analysis of a Cycloidal Wave Energy Converter Using Unsteady Reynolds Averaged Navier-Stokes Simulation}},
year = {2013}
}
