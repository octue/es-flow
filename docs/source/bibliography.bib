@article{Colucci2013,
abstract = {This paper looks at two aspects based on field data gathered by a single ADCP: 1) in situ characterisation of the wave and current fields at a wave energy site, and 2) a preliminary investigation into their interaction. A Teledyne RDI 5-beam ADCP with a vertical beam was used to record simultaneous measurements of waves and currents at 2Hz over the period of 2010-2011 at the UK's Wave Hub site where the water depth is 40m. It is found that the vertical beam accurately measures wave spectra up to high frequencies as well as providing directional wave data. Plots of wave parameters are presented for the deployment. The current velocity field exhibits bi-directional behaviour between ebb and flood phases. A preliminary investigation into the interaction of waves and currents at such a relatively deep site is undertaken and the methodology described. It is found that linear wave theory is applicable at the site, but collected data has proved difficult to notice important interactions. Some initial results are presented for the effects of waves on currents and for the effects of currents on waves. Some correlation was seen between the changes in the wave height according to the phase of the tide while the mean current profiles can be affected by the wave induced Stokes drift velocity. Even initial field data results like the ones presented in this paper are vital for future design and demonstration of wave energy converters. Despite inconclusive findings and the need for further study, a methodology for investigating wave current interactions from ADCP field data at a relatively deep site is presented.},
author = {Colucci, Antonella M and Bouferrouk, Abdessalem and Hardwick, Jon P and Johanning, Lars},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Colucci et al. - 2013 - Characterising Wave-Current Fields and their Interaction from in situ Measurements.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {Stokes drift,mean current profiles,modulation,wave current interactions,wave parameters,wave spectra},
title = {{Characterising Wave-Current Fields and their Interaction from in situ Measurements}},
year = {2013}
}
