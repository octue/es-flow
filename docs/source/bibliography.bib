volume = {AIAA 2005},
year = {2005}
}
@article{Gupta2006,
abstract = {A time-accurate free-vortex wake model has been used to predict the$\backslash$nskewed wake geometry behind an upwind$\backslash$n$\backslash$nwind turbine in yawed flow over a range of yaw angles and tip speed$\backslash$nratios. The predicted tip vortex positions$\backslash$n$\backslash$ncompared well with flow visualization measurements of the tip vortex$\backslash$npositions found in the experiments. Increasing$\backslash$n$\backslash$nwake skewness was found to cause the wake to roll up along its edges.$\backslash$nGood agreement was achieved between the$\backslash$n$\backslash$npredicted and measured turbine thrust for the entire range of conditions$\backslash$nused in the experiment.},
