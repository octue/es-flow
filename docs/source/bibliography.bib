keywords = {computational fluid,current energy conversion,marine hydrokinetic,performance testing,reference models},
title = {{US Department of Energy (DOE) National Lab Activities in Marine Hydrokinetics: Scaled Model Testing of DOE Reference Turbines}},
url = {http://mhk.pnl.gov/wiki/images/9/9d/Ruehl{\_}Presentation{\_}EWTEC{\_}2013.pdf},
year = {2013}
}
