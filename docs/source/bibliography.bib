@article{Siegel2013,
abstract = {A cycloidal wave energy converter (CycWEC) is a novel wave energy converter design which uses lift based hydrofoils as a means of interacting with ocean waves, as well as feedback control to terminate the incoming wave. The hydrofoils are attached at a distance to the main shaft, and the device converts wave energy to shaft power directly by rotating in sync with the incoming wave. The particular double cycloidal wave energy converter configuration investigated here consists of two of these devices arranged symmetrically on a common shaft. Since the generator and both devices are attached to the ocean floor by means of a central monopile, there is a gap between the left and right device to allow attachment of the double wave energy converter assembly to the ocean floor. In this publication we investigated the impact of the gap between left and right device, as well as overall device span on the three dimensional radiation efficiency. This was done using a numerical 3D radiation simulation code that was validated in earlier work against experimental data from 1:10 scale wave tank tests. For small blade spans the gap was found to cause an increase in wave energy converter efficiency, while for very large spans the efficiency decreased with gap size. Inspection of the radiation patterns yielded an understanding of the flow physics responsible for the efficiency changes.},
author = {Siegel, Stefan G and Fagley, Casey and Seidel, J{\"{u}}rgen and Jeans, Tiger},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Siegel et al. - 2013 - 3D Wave Radiation Efficiency of a Double Cycloidal Wave Energy Converter.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {Cycloidal Wave Energy Converter,Efficiency,Hydrofoil,Radiation,Wave energy},
title = {{3D Wave Radiation Efficiency of a Double Cycloidal Wave Energy Converter}},
year = {2013}
}
