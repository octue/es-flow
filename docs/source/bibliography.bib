@article{OCathain2013,
abstract = {Energy production estimate (EPE) uncertainty categories developed for onshore wind energy have good application when applied to wave and tidal energy. In broad terms wave and tidal EPE uncertainty is split between uncertainty in the resource before it meets the marine energy converter (MEC), and uncertainty due to energy losses between the MEC and the electricity metering point. In order to combine these two uncertainties, the resource uncertainty must be converted to an equivalent energy uncertainty, using sensitivity coefficients. This paper examines the characteristics of sensitivity coefficients for various combinations of resource levels in combination with various (nominal) MEC power functions, for both wave and tidal energy converters. Further the paper explores the practical implications on MEC design when setting MEC power ratings to minimize sensitivity coefficients for a given resource. For context, the paper initially considers the theory of error propagation, and particular consideration is given to the validity of an assumption, prevalent in wind EPEs, of normally distributed errors on input variables.},
author = {{\'{O}}'Cath{\'{a}}in, M{\'{i}}che{\'{a}}l and Carnus, Tim and Stock-Williams, Clym},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/{\'{O}}'Cath{\'{a}}in, Carnus, Stock-Williams - 2013 - Wave {\&} Tidal Energy Production Estimates Converting Resource Uncertainties into Energy Unc.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {P90/P50,coefficients,combining uncertainties,energy production estimates,financial risk.,sensitivity},
title = {{Wave {\&} Tidal Energy Production Estimates : Converting Resource Uncertainties into Energy Uncertainties}},
volume = {4},
year = {2013}
}
