@article{Karikari-Boateng2013,
abstract = {Reliability of an emerging technology is critical to the success of the technology. One of the emerging technologies for extracting offshore renewable and sustainable energy is the horizontal axis tidal current turbine. They are, in term of configuration and technology, very similar to medium size horizontal axis wind turbines. Due to their similarity it is possible and reasonable to use experience in the wind industry to investigate the potential reliability issues that may occur in the tidal industry. One of the persistent reliability issues in the wind industry is the failure of wind turbine drivetrains. Design deficiencies caused by a lack of in-depth understanding of the operating conditions had been suggested as the possible cause of failures. It is therefore critical that the operating conditions of tidal current turbines are thoroughly understood. Understanding and quantifying the loads will play an important role in assessing the reliability of tidal turbine designs. This paper demonstrates the influence of tidal turbulence on non-torque loads in the main shaft of a tidal turbine. Loads in the main shaft are transmitted through the drivetrain hence some common failure modes which may occur in the drivetrain are discussed.},
author = {Karikari-Boateng, K. a. and Ng, C and Grimwade, J and Johanning, L and Mueller, M a and Barltrop, N},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Karikari-Boateng et al. - 2013 - Reliability of Tidal Turbines using Wind Turbine Experience.pdf:pdf},
journal = {10th European Wave and Tidal Energy Conference (EWTEC 2013), Aalborg, Dk, 02 - 05 Sep 2013},
keywords = {drivetrain,failure modes,in new technology developments,it is common practice,little reliability information that,non-torque loads,reliability,surrogate,the structure,tidal turbine,where there is very,wind turbine},
title = {{Reliability of Tidal Turbines using Wind Turbine Experience}},
year = {2013}
}
