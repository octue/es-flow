keywords = {corner singularity; finite element; stress intensi},
month = {mar},
number = {1},
pages = {286--299},
publisher = {SIAM PUBLICATIONS},
title = {{A Finite Element Method Using Singular Functions for the Poisson Equation: Corner Singularities}},
