@article{Koo2013,
abstract = {Hydrodynamic performance of a floating Oscillating Water Column (OWC) wave energy converter, Backward Bent Duct Buoy (BBDB) was evaluated by using a two-dimensional fully-nonlinear Numerical Wave Tank (NWT) technique. The specially devised NWT for BBDB time-domain simulation was based on the potential theory and the boundary element method (BEM) with constant panels on the boundaries. The Mixed Eulerian-Lagrangian (MEL) approach was adopted to capture the nonlinear free surfaces inside or outside the chamber. A special numerical treatment was applied on the chamber free surface boundary condition in order to consider the viscous energy loss due to BBDB shape and large motions. In order to realize the open-sea condition a special artificial damping scheme was applied to both the frontal area near the wave maker and the end-beach area. The damping scheme can dampen reflected waves and preserve incident waves. The free surface elevations inside or outside the chamber, interacted with a tuned viscous damping coefficient, were calculated for various incident wave conditions. Effects of body motions on the strength of viscous energy loss were investigated and proper damping coefficients were selected from the comparison of experimental data.},
author = {Koo, WeonCheol and Kim, Sung-Jae},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Koo, Kim - 2013 - Nonlinear Time-Domain Simulation of Backward Bent Duct Buoy ( BBDB ) Floating Wave Energy Converter.pdf:pdf},
