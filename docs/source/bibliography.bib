@article{Robins2012,
abstract = {Regions that are favourable for tidal-stream energy extraction generally have high-velocities and low sediment accretion. However, it may be the case that energy extraction at these locations affects three important sediment transport processes: (1) fine and suspended sediment concentrations (turbidity maxima), (2) local sediment transport and the formation of sand banks, and (3) shelf-scale net sediment fluxes. Tidal energy extraction and sediment dynamics are simulated at a high-velocity location off the northwest coast of Anglesey, Wales (UK). This site generates some of the largest tidal velocities in the Irish Sea ({\textgreater} 2.5 m s-1, during spring tidal flow), due to high tidal amplitudes and flow being constricted by a collection of small rocky islands known as The Skerries. This site has been highlighted as one of seven specific regions of interest around the UK for ‘first generation' tidal energy extraction and has been leased by the Crown Estate for commercial development [1]. Tidal dissipation here generates a turbidity maximum; important as a source of secondary production and as a nursery area for marine species. Sand banks also form in the lee of headlands and islands and act as natural coastal protection. Flow around the northwest coast of Anglesey is asymmetrical in character and generates a net flux of coarse sediment into Liverpool Bay. In this paper, it is investigated whether energy extraction at the Skerries will impact these morphodynamic processes in a significant way, relative to seasonal or inter-annual variability.},
author = {Robins, Peter E},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Robins - 2012 - Influence of tidal-stream energy extraction on sediment dynamics.pdf:pdf},
