@article{Markus2013,
abstract = {In recent years, the successful realization and operation of tidal turbine prototypes has increasingly confronted engineers with marine environments that consist of high current velocities that may occur simultaneously with large wave scenarios. The complex flow field associated with such conditions makes the design of offshore foundations a particularly challenging task. In this paper, numerical methods based on Computational Fluid Dynamics are described and applied to a shape optimal design study of a gravity base foundation. The structure is analyzed for pure wave, pure current, and combined wave-current-loading. It is shown that the optimal design with respect to a minimization of horizontal forces may vary depending on the prevailing marine environments at the offshore site. Furthermore, attention is drawn to the influence of wave-current interaction on the resulting forces. Under certain conditions, the data predict a significant load increase compared to a linear superposition of pure wave and pure current-loading. It is shown that a great optimization potential for reliable structures is gained when the predominant offshore conditions are modeled accurately.},
author = {Markus, D and Bletzinger, K-U},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Markus, Bletzinger - 2013 - Optimal Shape Design of Offshore Foundations Subjected to Wave and Current Loading.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {computational fluid,dynamics,gravity base foundations,optimal shape design,tidal turbines,wave-current interaction},
title = {{Optimal Shape Design of Offshore Foundations Subjected to Wave and Current Loading}},
year = {2013}
}
@article{Marquis2013,
abstract = {Although many wave energy projects have been designed only a few of them have been tested with success under real conditions. Over the last three years Wavestar has systematically collected and published real-sea operational and performance data from the Wavestar demonstrator in the North Sea, giving an indication of the achievements of the demonstrator in three main criteria of success: predictability, performance and reliability. Currently Wavestar focuses on the development of offshore wind/wave farms with bottom fixed foundations in water depths of 10 to 30 metres. However, on the longer run the wind turbine technology is expected to develop for use in deeper waters using floating foundations, and when this industry is mature Wavestar will start developing floating wave energy converters. The combination of Wavestar converters and offshore wind turbines offers attractive energy prices. Energy production from a wind/wave farm is increased and power fluctuations are reduced which increases the value of the produced energy. Further potential synergies of combining wind and wave energy in the same area include cost savings e.g. by sharing infrastructure costs as well as operations and maintenance facilities. This paper describes the vision and plans for the introduction of the Wavestar wave energy converter concept on the European market. A business strategy through a consortium or other form of collaboration with industrial partners, investors and customers is essential for a successful market breakthrough. Further, the factors influencing the economic strategic model and political aspects are discussed as industrial growth and employment are the key success factors for the implementation of a new innovative industry.},
