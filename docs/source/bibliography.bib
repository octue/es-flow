@article{Pans2013,
abstract = {Exploration of marine energy resources in coastal areas is often in conflict with other users in the increasingly congested coastal marine environment. Marine energy converters competes for space against other interests such as navigation, fisheries, recreation, environmental protection, aquaculture or other industry. These factors create a complex system within which the merit of renewable energy development has to be evaluated. The cost-benefit calculations for the development of marine energy needs to consider not only these factors, but also take into account local and international legislation, existing land and ocean space use on top of the CAPEX and OPEX. Assessing these often conflicting factors can be a complex and time consuming part of a project, but also one that can be phased efficiently from early feasibility throughout project development. DHI and ERI@N are presenting here a framework with a set of methodologies to ensure that competing spatial processes can be captured and assessed appropriately to ultimately allow the usage of the ocean space to be optimised. DHI has developed a set of tools for tidal and wave energy primarily, expandable to other ocean space utilisation. The tools are combined in the MIKE Customised platform for stakeholders to meet, exchange ideas and investigate options. It is serving as a database, but also allows visualisation of options, analysis of cumulative impacts and scenario management. The approach provides an overview of the available resources, while taking into account the limitations in space and through that provides a basis for an informed decision between developers, investors and regulators. In the same line of thought, DHI as member of SI Ocean works to quantify the size of the market opportunity in order to attract investors to the marine renewable market, to optimise the resource access and to make the overall process more transparent. An example of project development within these constraints is the development of a small scale tidal array in the highly congested waters of Singapore. The island Pulau Semakau is currently used as a testbed for wind power and smart-grid storage solutions. However, the mixed tidal stages around Semakau are suitable for small scale tidal energy converters. This project, currently in development, will demonstrate that with careful planning and analysis it is possible to establish an array even in Singapore's competitive marine space. Besides navigational issues, the array will be close to an aquaculture facility, to cooling water intakes and outfalls, and to one of the few publicly accessible coral reefs in Singapore. The online decision support tool has paved the way for this milestone project through presenting an integrated view of the highly detailed understanding of the driving local physical conditions, combined with high resolution spatial and temporal maps of the marine ecosystem, industrial receptors, and the all-important navigational requirements for shipping. One additional benefit of using this integrated approach is that it can be iterated endlessly as more information becomes available, and therefore the decision makers can track the progress of the resources throughout the project lifecycle from feasibility to energy forecast, along with the implications on environment and cost.},
author = {Pans, Sonja and Rasch, Peter and {Sim Abundo}, Michael Lochinvar and Petersen, Ole Svenstrup},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Pans et al. - 2013 - Making Space for Offshore Renewables - Spatial Planning for Marine Energy.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {Marine Energy,Resource Assessment,Spatial Planning,customised tool,numerical modelling},
title = {{Making Space for Offshore Renewables - Spatial Planning for Marine Energy}},
year = {2013}
}
@article{Papo2013,
abstract = {Wave energy development is gradually moving toward the first installation of both testing sites in Europe and also some full scale wave energy devices at open sea connected to the grid. Most devices currently under development are floating structures requiring some kind of station-keeping systems to operate, and so far the mooring system has been considered as a simple add-on component of the structure, and not as a structural element itself. The purpose of this paper is to provide further knowledge in the analysis of the mooring system for Wave Energy Converters, taking into account not only the mechanical behaviour of mooring system, but also providing an economic insight of the problem and achieving the purpose to optimize the energy absorption. Indeed, this paper presents the design and the techno-economic analysis of a mooring configuration for an axisymmetric heaving device. The design has been carried out by using a numerical model based on a quasi-static approach. The evaluation of the mooring system performance is based on existing normative and previous. The global cost, taking into account the main elements that constitute the system, has been estimated for several configurations of the mooring system, and the dependence of the costs on several variables analysed.},
author = {Papo, F Boscolo and Para, B De Miguel and Mendia, J L{\'{o}}pez and Ojanguren, M and Rico, A},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Papo et al. - 2013 - The economic implications of the design of mooring systems for wave energy converters.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {cost,evaluation,mooring stiffness,mooring system,offshore marine chain,wave energy converter},
title = {{The economic implications of the design of mooring systems for wave energy converters}},
year = {2013}
}
@article{papoulis1975,
abstract = { If only a segment of a function{\textless}tex{\textgreater}f (t){\textless}/tex{\textgreater}is given, then its Fourier spectrum{\textless}tex{\textgreater}F(omega){\textless}/tex{\textgreater}is estimated either as the transform of the product of{\textless}tex{\textgreater}f(t){\textless}/tex{\textgreater}with a time-limited window{\textless}tex{\textgreater}w(t){\textless}/tex{\textgreater}, or by certain techniques based on various a priori assumptions. In the following, a new algorithm is proposed for computing the transform of a band-limited function. The algorithm is a simple iteration involving only the fast Fourier transform (FFT). The effect of noise and the error due to aliasing are determined and it is shown that they can be controlled by early termination of the iteration.  The proposed method can also be used to extrapolate bandlimited functions.},
