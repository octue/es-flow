@article{Bauer,
abstract = {The specific speed (type number) is a characteristic number of fluid flow machines, which is based upon their design parameters. Specific speed widely serves as an indicator of selecting and designing conventional turbines, such as Kaplan turbines. It is also common to design tidal turbines in using theories that were originally developed for wind turbines. In so doing, only characteristic numbers of wind turbines are calculated, however not their specific speed. Nevertheless, tidal turbines are fluid flow machines too. Thus comparisons between their specific speed and that of other fluid flow turbines are possible but have yet to be done. In analysing the performance of tidal turbines and concepts thereof, this article calculates the specific speed and other characteristic numbers. The results are compared to those of conventional turbines as well as wind turbines and then integrated to conventional diagrams. It can be shown that tidal turbines are associated with high values of specific speed so that their data lies outside the range of conventional diagrams. Compared to wind turbines, the specific speed of tidal turbines is high too, although not as high as that of wind turbines. This article closes in indicating that numerous tidal turbines, whose design is based on wind turbines, should be indeed designed with higher values of specific speed. Diagrams of specific speed for conventional turbines should be extended to further integrate tidal turbines. Keywords},
author = {Bauer, A. and Thamsen, P. U.},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Bauer, Thamsen - Unknown - The specific speed of performing tidal turbines and concepts thereof.pdf:pdf},
journal = {10th European Wave and Tidal Energy Conference (EWTEC 2013), Aalborg, DK, 02. - 05. Sep 2013},
keywords = {capacity of 1m³,conditions and reaches a,e,fluid flow machine,i,its operating point,maximal efficiency,of 1m at,s and a head,specific speed,the specific,thus,tidal turbine,turbine,type number,water,wind turbine},
title = {{The specific speed of performing tidal turbines and concepts thereof}}
}
