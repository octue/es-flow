@article{Buckland2010,
abstract = {Blade Element Momentum Theory (BEMT) is a method of modelling the performance of wind, current or tidal turbines. It is split into two parts, one dimensional momentum theory with rotational momentum and blade element theory. When modelling the turbines out of the normal operating range the most commonly used convergence method of the BEMT expressions for the axial thrust and rotational torque is shown to be problematic. An alternative, more robust, scheme is discussed which is based on a combined Monte Carlo and sequential quadratic optimisation of the derived BEMT equations. Tip and hub losses are considered and Prandtl's corrections are implemented. These corrections are compared to other tip and hub loss methods and for differing angles of incoming flow. The torque, axial force and power coefficients found using this method are compared to a lifting line theory model and another BEMT code for various Tip Speed Ratios. It is shown that the method, with tip and hub losses, agree well with the alternative BEMT scheme. Both these programs predict consistently higher values than the lifting line theory method as this, more conservative, approach accounts for the three dimensional flow effects such as the radial flow created by the rotation of the blades.},
author = {Buckland, H. C. and Masters, I. and Chapman, J.C. and Orme, J.a.C.},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Buckland et al. - 2010 - 1 Blade Element Momentum Theory 2 Numerical Implementation.pdf:pdf},
journal = {18th UK Conference on Computational Mechanics (ACME-UK)},
keywords = {blade element momentum theory,optimization function,tidal turbine},
number = {March},
pages = {1123--1127},
title = {{1 Blade Element Momentum Theory 2 Numerical Implementation}},
