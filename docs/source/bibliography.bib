keywords = {atmospheric turbulence,wind measurements,wind turbine design code simulations,wind turbine performance},
pages = {17},
title = {{The Impact of Coherent Turbulence on Wind Turbine Aeroelastic Response and Its Simulation}},
year = {2005}
}
