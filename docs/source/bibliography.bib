@article{Vickers2013,
abstract = {As marine renewable energy developers aim to scale up their devices, the greater removal of energy from propagating waves increases the potential for environmental impacts at the coastline. Quantifying the impact of energy extraction on sensitive areas has proved a contentious issue and a source of ill feeling between developers and stakeholders. However, local stakeholders concerned about the potential impacts of marine renewable energy developments are often connected to specific coastal locations. These include ecologically sensitive areas, and recreational areas such as those used for surfing. Careful consideration of device placement may limit the severity of impacts on these locations. Rather than quantifying the impacts on the coastline, this paper demonstrates an analytical method for calculating exclusion windows for coastal features using a surfing case study from the West coast of Orkney. Geographical areas through which waves that impact on sensitive areas are highlighted, and the implications of avoiding development in these areas are discussed. This methodology is seen as a tool to allow constructive discussions between stakeholders and developers, enabling WEC devices to coexist near sensitive coastal climates, and circumventing a contentious issue that can hinder project development.},
author = {Vickers, A and Ashton, I G C and Smith, H C M and Nieuwkoop, J Van and Johanning, L},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Vickers et al. - 2013 - An Analytical Method for Calculating Wave Energy Converter Exclusion Windows for Coastal Features.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {array optimization,shoreline impact,surfing,swan,wave modelling},
title = {{An Analytical Method for Calculating Wave Energy Converter Exclusion Windows for Coastal Features}},
year = {2013}
}
