@article{Child2013,
abstract = {In this paper, a modified third generation spectral wave solver (TOMAWAC) is used to model the power absorption and evolution of wave energy spectra across wave farms. This, unlike many other solvers, has the advantage of being able to consider variable site bathymetry, wave absorption and some nonlinear wave processes in an acceptable computational time frame. This paper presents comparisons between numerical estimates and tank test results, conducted as part of the PerAWaT project. The experiments addressed the performance of large arrays of point absorber WECs (up to 24 units) and were performed with 1:80 scale models at Queen's University Belfast (QUB). Numerical simulations were also conducted with a frequency-domain model in order to verify the results derived from the spectral solver. Comparisons between numerical predictions and experimental results for a variety of wave conditions and different array layouts are presented. Key performance variables such as absorbed power per sea state for each WEC as well as overall ‘interaction factor' are considered. Hydrodynamic array interactions were found experimentally to cause losses of up to 26{\%} in energy yield, compared to WECs operating in isolation. The spectral-domain model predicts losses reasonably well although the comparison is expected to improve under more realistic conditions.},
author = {Child, B F M and Weywada, P Laporte},
