@article{Lehmann2013,
abstract = {It has been reported worldwide that muddy seafloors can extract a significant fraction of the energy stored in overpassing ocean waves within several wavelengths. We consider a synthetic-seabed-carpet, supported bymultiple springs and generators, that should extract wave energy the same way as muddy seafloor does. This paper presents the analytical model, the construction of the first working prototype and the experimental results of this novel concept called the Carpet of Wave Energy Conversion (CWEC).We successfully demonstrated its capability to absorb and convert wave energy for multiple sea states. As the CWEC operates completely submerged, it imposes no threat for overpassing ships and has no effect on oceanfront scenes. The CWEC's orientation combines the advantages of a point absorber, an attenuator and a terminator: it is wave- direction-independent, has a high absorption efficiency and can be exposed to high wave energy flux with its flexible spread perpendicular to the wave-propagation direction. The ability to cancel waves can be used to create safe zones in the ocean, prevent erosion and protect harbors. Secondly, the CWEC's primary energy conversion creates seawater at high pressure. This can be used to generate electricity, be used for desalination and for the distribution of fresh water. High pressure water can supply pumped-storage hydroelectric power plants, which are the most efficient way to balance the electrical grid.},
author = {Lehmann, Marcus and Elandt, Ryan and Pham, Henry and Ghorbani, Reza and Shakeri, Mostafa and Alam, Mohammad-Reza},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Lehmann et al. - 2013 - An artificial seabed carpet for multidirectional and broadband wave energy extraction Theory and Experiment.pdf:pdf},
journal = {10th European Wave and Tidal Energy Conference},
title = {{An artificial seabed carpet for multidirectional and broadband wave energy extraction: Theory and Experiment}},
url = {http://taflab.berkeley.edu/wp-content/uploads/2014/01/EWTEC2013CWEC -TAFLAB.pdf{\%}5Cnhttp://www.ewtec.org/ewtec2013},
year = {2013}
}
