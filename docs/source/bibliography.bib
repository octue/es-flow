@article{Simas2013,
abstract = {The harnessing of wave energy covers a wide range of technologies, most of them still under concept testing. Although several projects have already been installed at sea, especially in European waters, the Environmental Impact Assessment is still regarded as a non-technological barrier. Different consenting regimes are in place or are expected for test centres where ocean energy projects have been / will be installed for demonstration. In general, some of these European test centres are pre-consented, meaning that developers do not have to go through a full consenting process; providing a limited number of permit requirements are fulfilled including an environmental appraisal (usually less onerous than a full EIA). The environmental characterization for some of these wave energy test centres (AMETS in Ireland, bimep in Spain, Lysekil in Sweden, Ocean Plug in Portugal, SEM-REV in France and Wave Hub in England) exist and has been compiled in a Data Management Platform (DMP) as part of the SOWFIA project (www.sowfia.eu/). This information can give input on critical environmental descriptors and allow the comparison between them in order to identify commonalities and differences in the environmental characterization. This paper will provide, as far as possible, conclusions on environmental test centres sensitivities and assessment.},
author = {Simas, T and O'Callaghan, J and Saulnier, J B and Marina, D and Magagna, D and Bailey, I and Greaves, D and Embling, C and Sundberg, J},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Simas et al. - 2013 - Identification of critical environmental and socio- economic issues of designated wave energy test centres across.pdf:pdf},
