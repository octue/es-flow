journal = {Physics of Fluids},
month = {feb},
number = {2},
pages = {1024},
publisher = {AMER INST PHYSICS},
title = {{Wall turbulence closure based on classical similarity laws and the attached eddy hypothesis}},
type = {Proceedings Paper},
url = {http://scitation.aip.org/content/aip/journal/pof2/6/2/10.1063/1.868336},
volume = {6},
year = {1994}
}
