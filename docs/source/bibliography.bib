@article{Fleming2013,
abstract = {The effect of flow shear on turbine performance and wake development is examined. A computational model of an axial flow tidal turbine is developed and validated, and performance evaluated in sheared velocity profiles representative of a tidal channel. The computational model is developed in the commercial flow solver ANSYS Fluent. The rotor is resolved with a body-fitted grid, which is allowed to rotate relative to the channel boundaries. Lateral boundary conditions are applied so as to simulate an infinite row of turbines. The free surface of the flow is modelled as a rigid lid, by applying a slip condition at the upper surface of the domain. A sheared velocity profile is defined at an upstream boundary and maintained throughout the channel through the specification of shear stress at the channel bed. Comparison of the model against scale turbine experiments is reported. The device is simulated at various vertical positions in a sheared flow. Rotor thrust and power are observed to increase with elevation above the channel bed. This is a consequence of the higher kinetic flux available in upper portions of the velocity profile. Further, it is found that using an alternative normalisation for thrust and power, based on the area average of the square and cube of the upstream velocity profile, enables collapse of results across a range of sheared profiles.},
author = {Fleming, C. and McIntosh, S. and Willden, R.},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Fleming, McIntosh, Willden - 2013 - Tidal turbine performance in sheared flow.pdf:pdf},
journal = {10th European Wave and Tidal Energy Conference},
title = {{Tidal turbine performance in sheared flow}},
year = {2013}
}
