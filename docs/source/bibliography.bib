@article{Torre-Enciso2013,
abstract = {Located in the Basque Country near the city of Bilbao, the Biscay Marine Energy Platform (bimep) is an open-sea facility to support research, technical testing and commercial demonstration of pre-commercial prototype utility-scale floating Marine Renewable Energy Devices (MREDs). Bimep has identified the need to promote high standards of safety to protect the marine environment and nearby navigational routes in addition to bimep's infrastructure, developers or subcontractors operating on the site, and the device assets themselves. This paper presents a methodology based on a set of high level technical requirements to be considered by MRED developers prior to being granted acceptance to deploy their equipment within the facility. The resulting safety considerations for the deployment of MREDs at bimep are then applied to one particular case study, the Oceantec Wave Energy Converter. The risk assessment philosophy provides some important benefits for MRED developers, namely enhanced device design and construction, better operational and contingency plans, improved communication with bimep (particularly when changes affect safety), and clearer distribution of responsibilities.},
author = {Torre-Enciso, Yago and Ruiz-Minguela, Pablo and Rico, Antonio and Marina, Dorleta},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Torre-Enciso et al. - 2013 - Safety Considerations for the Deployment of Marine Renewable Energy Devices at Biscay Marine Energy Platfor.pdf:pdf},
