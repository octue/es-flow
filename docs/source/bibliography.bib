@article{Mackay2013,
abstract = {Tests with an array of four Wavebob point absorber -type wave energy converters (WECs) are compared to numerical predictions from a time-domain modelling tool for WECs, called WaveDyn. The test programme comprised radiation tests, where the power take-off (PTO) of one model was excited in initially calm water and the effects of the radiated waves on the other models were measured, and also tests in regular and irregular waves. The radiation tests showed that the effect of reflected wave components (due to the presence of the tank walls) on the models was of a similar magnitude to the array interactions. Comparisons with WaveDyn results showed that the effects of radiated waves could be accurately reproduced. The regular wave tests were analysed in a way that excludes the effect of reflected waves, so that only clear array interactions are visible. Comparisons showed good agreement between the tank and numerical results at most frequencies. The irregular wave tests exhibit a smaller level of array interactions due to the combined positive and negative effects of waves at multiple frequencies. The effect of reflected waves on the comparisons between tank and numerical results in irregular waves is shown to have a significant effect on the uncertainty.},
author = {Mackay, Ed and Cruz, Joao and Livingstone, Michael and Arnold, Peter},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Mackay et al. - 2013 - Validation of a Time-Domain Modelling Tool for Wave Energy Converter Arrays.pdf:pdf},
journal = {Proc. of the 10th European Wave and Tidal Energy Conf., Aalborg, Denmark},
keywords = {1,2,3,and other device concepts,array interactions,domain numerical model,extends previously documented experiments,performance prediction,tank testing,time-,using isolated,validation,wavebob models},
