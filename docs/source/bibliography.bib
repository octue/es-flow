@article{alfonsi2006,
abstract = {In this paper the issue of the coherent structures of turbulence developing$\backslash$nin wall-bounded flows is addressed. After a short historical synthesis,$\backslash$nsome basic concepts are reviewed and the idea of coherent structure$\backslash$nis introduced. The phenomena occurring in the inner and outer regions$\backslash$nof the turbulent boundary layer in conjunction with the most widely$\backslash$nused event-detection techniques are considered, with reference to$\backslash$nthe large amount of mainly experimental results existing on the subject.$\backslash$nThe flow phenomena are described in terms of events occurring in$\backslash$nthe inner region, large-scale motions developing in the outer layer$\backslash$nand dynamics of vortical structures. In the second part of the paper,$\backslash$nmethods for the eduction of the coherent structures of turbulence$\backslash$nfrom the background flow and results obtained in the framework of$\backslash$neach method are presented. The techniques involving the invariants$\backslash$nof the velocity gradient tensor, the analysis of the Hessian of the$\backslash$npressure and the proper orthogonal decomposition are considered.$\backslash$nEach procedure involves a particular definition of coherent structure$\backslash$nthat is supported by an appropriate mathematical framework and permits$\backslash$nthe analysis of a turbulent-flow database in terms of dynamics of$\backslash$ncoherent structures. This work may contribute to the dissemination$\backslash$nof the most recent concepts and techniques now in use in turbulence$\backslash$nresearch among fluid dynamicists.},
author = {Alfonsi, Giancarlo},
doi = {10.1115/1.2345370},
issn = {00036900},
journal = {Applied Mechanics Reviews},
number = {6},
pages = {307},
title = {{Coherent Structures of Turbulence: Methods of Eduction and Results}},
volume = {59},
year = {2006}
}
