@article{Bossanyi2012,
abstract = {Recent developments in LIDAR technology have led to a great deal of interest in the possibility of improving turbine control by making use of a turbine-mounted Lidar system to sense the approaching wind field before it reaches the turbine, providing preview information which might help the controller to improve turbine performance. This could significantly reduce the loads on the turbine, leading to improved cost-effectiveness, especially for large turbines. There have also been some claims of direct increases in energy capture as a result of using such preview information. Simulation models are a vital tool for assessing these claims, for developing control algorithms which can make use of the Lidar signal, and to help Lidar developers optimise their systems for this application. Current simulation models make use of Taylor's frozen turbulence hypothesis, but this is not strictly valid when Lidar is used to measure wind speeds upstream of the turbine, as the measured wind cannot be assumed to convect unchanged to the turbine. In this paper, a method for avoiding the frozen turbulence assumption is proposed, and some simulation results are presented to illustrate the effect this might have on the fatigue load reductions which could be achieved using Lidar-assisted control. The paper also discusses some issues surrounding the possibility of extreme load reduction by using Lidar to sense approaching gusts. The standard specifications for extreme gusts should be reviewed if Lidar preview information is to be used by the controller.},
author = {Bossanyi, E},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Bossanyi - 2012 - Un-freezing the turbulence improved wind field modelling for investigating Lidar-assisted wind turbine control.pdf:pdf},
journal = {Proceedings of EWEA 2012},
keywords = {and extreme loads on,control,if the cost-,indicate that,lidar,loads,some of these studies,strategies,the turbine,there is significant potential,to reduce both fatigue,wind turbine},
title = {{Un-freezing the turbulence: improved wind field modelling for investigating Lidar-assisted wind turbine control}},
year = {2012}
}
