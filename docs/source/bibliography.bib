@article{Gleizon2013,
abstract = {A vast programme to exploit waves and tidal energy in Scottish waters is underway. The characterisation of nearshore waves in Scottish waters is complicated by the jagged coastline and archipelagos, and strong wave-current interactions in locations such as the Pentland Firth. In order to investigate the energy availability, the Environmental Research Institute has implemented the spectral wave model SWAN on an unstructured grid covering a large domain, including all potential energy sites, allowing long fetch distance ({\textgreater}600 km) and down to a fine resolution nearshore (50 m). Wave data were collected at two locations in the Pentland Firth from wave rider buoys during the first six months of 2012, completing Wavenet data near the Outer Hebrides and in Moray Firth. Although under-predicting some of the peak values, the model predictions generally reproduced the observed time series effectively at the four locations, with Pearson correlation coefficient r between 0.73 and 0.84 between predicted and measured time series for all sites. The seasonal and yearly wave power estimates were broadly consistent with previous assessments. The dominant wave energy is located on the western coasts with wave power reaching 200 kW/m during winter. An improved representation of ocean swell should improve future versions.},
author = {Gleizon, Philippe and Woolf, David},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Gleizon, Woolf - 2013 - Wave Energy Assessment in Scottish Seas.pdf:pdf},
journal = {EWTEC 2013 Proceedings},
keywords = {scottish waters,seasonal variations,swan model,wave energy,waves modelling},
title = {{Wave Energy Assessment in Scottish Seas}},
year = {2013}
}
@article{Goddjin2012,
abstract = {AbstractNumerous acoustic Doppler current profiler (ADCP) surveys were performed in the Inner Sound of the Pentland Firth, a channel between the Orkney Islands and the northern coast of Scotland connecting the Atlantic Ocean to the west and the North Sea to the east. The Pentland Firth has the highest tidal streams of the British Isles, and one of the highest that can be found around the globe. Here, the tidal energy industry is in its demonstration phase, but not many real current measurements are in the public domain. The authors present real current data, measured during different phases of the tidal cycle, using a vessel-mounted ADCP. The tidal changes can be rapid, and because the underway measurements take time, the apparent spatial patterns are affected by temporal variation. A method is described that estimated and corrected this temporal distortion using a hydrodynamic model. It appeared that ebb and flood streams did not fully overlap, and that the tidal streams were more complicated, turbulent, and variable than existing models suggest. The data were analyzed for characteristics pertinent to practical tidal stream energy exploitation, and two favorable sites in the Inner Sound are identified. All original current data are available from the British Oceanographic Data Centre (BODC).},
annote = {doi: 10.1175/JTECH-D-11-00223.1},
author = {Goddijn-Murphy, Lonneke and Woolf, David K and Easton, Matthew C},
issn = {0739-0572},
journal = {Journal of Atmospheric and Oceanic Technology},
month = {aug},
number = {1},
