@article{Magar2013,
abstract = {The study reported in this paper focuses on the characterisation of wave energy resources of Guernsey's Western approach, within 12 nautical miles (NM) west of Guernsey Island, located within the English channel region, approximately 58 nautical miles south of the English coast and a few miles west of the Cherbourg peninsula (France). Wave conditions over 2 years (From January 2009 to December 2010) were produced using Delft3D-Wave (a version of SWAN embedded within the Delft3D modelling suite) at different locations around Guernsey. The model was forced with WaveWatchIII (WWIII) model data at a point with a latitude of 49.530, a longitude of -2.940, a depth of 77m, and at 3 hourly intervals for the 2-year study period. Model validations with data by the same WWIII model at a grid point with a latitude of 49.530, a longitude of -2.770, and a depth a 66m, at 3-hourly intervals and for the same 2-year period, showed an extremely good agreement between model predictions and WWIII data. The boundary conditions data and the model validation data were kindly provided by the Met Office for this particular project. A constant mean sea level was assumed. Computations of monthly and mean available power output throughout the study area for the 2-year period highlighted three regions, two near the 12NM limit, and one very near the Island itself, where maximum mean power outputs were of the order of 24 kW/m. This mean power output is similar to that at the WaveHub site, off the north coast of Cornwall, being developed as a Wave Energy test facility due to its excellent wave energy conversion potential.},
author = {Magar, V and Joshi, C R and Williams, B G and Conley, D},
