@article{Moisel2013,
abstract = {Based on the bi-directional operating principle of the axial Wells turbine, a radial turbine was suggested by KENTFIELD in the 1980ies, but never investigated in detail. As compared to the axial Wells turbine advantages are thought to be reduced secondary flow effects and hence increased operating range and peak efficiency. This paper deals with a first numerical (RANS) investigation of aerodynamically almost equivalent threedimensional axial and radial lift-based blade cascades for bidirectional flow and a comparison to a two-dimensional cascade. The comparison reveals that the radial turbine has a potential of improved peak efficiency and stall-free operating range as compared to the axial Wells turbine. It turned out that the meridional contour in the vicinity of the radial blade cascade has a major impact on the performance. By controlling the meridional throughflow area one can achieve an almost similar performance as well as for centrifugal and centripetal flow direction. Comparing with the ideal 2D cascade one may conclude that there is still some potential for improving especially the operation range of the radial cascade.},
author = {Moisel, C and Carolus, T H},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Moisel, Carolus - 2013 - Comparison of axial and radial blade cascades for bi- directional wave energy air-turbines.pdf:pdf},
journal = {10th European Wave and Tidal Energy Conference},
keywords = {blade cascade,lift-based turbines,oscillating water column,radial turbine,turbine,wave power,wells},
pages = {2--8},
title = {{Comparison of axial and radial blade cascades for bi- directional wave energy air-turbines}},
year = {2013}
}
