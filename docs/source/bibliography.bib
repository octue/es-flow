@article{Schluntz2013,
abstract = {The performance enhancing effects of closely pack- ing tidal turbines in arrays are evaluated in this computational study. Various lateral spacings ranging from 0.25d to 4d (d is the rotor diameter), as well as a virtually unblocked case, are examined. Infinite turbine fences are simulated through the use of periodic boundary conditions. A blade element momentum method embedded in a three- dimensional Reynolds averaged Navier-Stokes solver (RANS- BEM) is applied to determine a hydrodynamically optimal rotor design for each lateral spacing, thus taking into account the effect of local blockage on rotor optimisation. Increased blockage is found to result in increased optimal rotor solidity and decreased optimal blade pitch. A matrix of array simulations is then completed with each rotor design tested for its design spacing as well as several off- design lateral spacings. The resulting power coefficient is found to be largest when the rotor optimised for the highest blockage case is used in the array with the closest lateral spacing. Further, even though a rotor's performance may be increased through operation at a blockage higher than its design point, it still exhibits inferior performance relative to a rotor designed for that higher blockage setting. The results indicate that the local blockage must be considered in the rotor design process if the optimal rotor efficiency for a given spacing is to be achieved.},
author = {Schluntz, Justine and Willden, Richard H J},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Schluntz, Willden - 2013 - The Effect of Rotor Design on the Power Output of Closely Packed Tidal Turbines.pdf:pdf},
