@article{Korde2013,
abstract = {This paper investigates wave energy conversion in open water where the goal is to utilize the wave-field focusing effect of a stationary disc submerged a short depth beneath the water surface. Dynamic interaction of the disc with additional coupled, submerged inertias is used to minimize its oscillation. The method used to enable and extend this favorable dynamic coupling is discussed here. An oscillating water column in a submerged duct attached under a small circular opening in the disc and driven by the wave-field over the disc is used for wave energy conversion. Non-real-time reactive control of the water column response to enhance energy absorption is examined. The focusing effect of the submerged stationary disc is confirmed with numerical calculations of surface elevation over the disc. Hydrodynamic coefficients and exciting forces in the surge, heave, and pitch modes are computed numerically. Calculations of hydrodynamic performance suggest that energy absorption from the oscillating water column is significantly greater under control holding the disc stationary, and can be improved further by applying reactive loads tuned to the optimal susceptance and conductance associated with the oscillating water column. Also studied in this paper is the effect of submergence depth on the radiation and exciting force impulse response kernels and the prediction horizon for real time reactive control.},
author = {Korde, Umesh a and Ertekin, R. C},
file = {:Users/thc29/Box Sync/Technical/Literature and Databooks/Mendeley/Korde, Ertekin - 2013 - An Open Water Submerged Device for Wave Energy Focusing and Conversion.pdf:pdf},
