@article{Walsh2004,
abstract = {Boundary layer transition is a critical parameter in the design of fluid flow systems. This$\backslash$nsituation is due to the dramatic change in both entropy production and heat transfer that$\backslash$naccompanies it. It is well recognized that many parameters affect the location of transition onset,$\backslash$nhowever, no models exist which unify all these parameters. This paper presents a new hypothesis$\backslash$nthat the driving force of boundary layer transition onset is the entropy generation rate alone, with all$\backslash$nother parameters being functions of this higher order quantity. At present this hypothesis is$\backslash$nspeculative, but encouraging since good compatibility is found with more established transition$\backslash$nmodels.$\backslash$n$\backslash$nKeywords: Entropy, Transition onset, Boundary layer stability},
